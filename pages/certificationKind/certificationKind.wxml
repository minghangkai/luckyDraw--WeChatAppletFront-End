  <navigator class="line" wx:for="{{objArray}}" wx:key="id"  target="" url="/pages/certificationInfo/certificationInfo" hover-class="navigator-hover" open-type="navigate">
    <image class="itemImage" src="{{item.imageSrc}}" mode="aspectFit" ></image>
    <view class="titleAndInfo">
      <text class="title">{{item.title}}</text>
      <text class="info">{{item.info}}</text>
    </view>
  </navigator>
